    #snackbar {
        visibility: hidden;
        min-width: 250px;
        margin-left: -125px;
        background-color: #2a23b3;
        color: #fff;
        text-align: left;
        font-weight: bold;
        border-radius: 2px;
        padding: 9px 16px;
        position: fixed;
        right: 20px;
        top: 30px;
        z-index: 9999;
        font-size: 17px;
      }
      
      #snackbar.show {
        visibility: visible;
        -webkit-animation: fadein 0.5s, fadeout 1s 2.5s;
        animation: fadein 0.5s, fadeout 1s 2.5s;
      }


      
      @-webkit-keyframes fadein {
        from {top: 0; opacity: 0;} 
        to {top: 30px; opacity: 1;}
      }
      
      @keyframes fadein {
        from {top: 0; opacity: 0;}
        to {top: 30px; opacity: 1;}
      }
      
      @-webkit-keyframes fadeout {
        from {top: 30px; opacity: 1;} 
        to {top: 0; opacity: 0;}
      }
      
      @keyframes fadeout {
        from {top: 30px; opacity: 1;}
        to {top: 0; opacity: 0;}
      }
