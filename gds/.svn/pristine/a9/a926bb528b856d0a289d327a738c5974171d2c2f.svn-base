.header-fixed { 
    position: absolute; 
    display:none;
    z-index: 1;
    overflow: hidden;
    margin-right: 15px;
    // width: auto;
}
.parent-tabl-container{
    position: relative;
}
table {
    margin-bottom: 0;
}
table thead{
    background-color: #187080;
    color: #FFF;
}
td{
    text-align: center;
}
table thead th{
    vertical-align: middle;
    text-align: center;
//    min-width: 75px;
}
.pagination-view
{
    float:right;
}
#myInput {
    width: 100%;
    border-radius: 0;
    font-size: 16px; 
    padding: 7px 20px 12px 30px;
    font-style: inherit;
    border: 1px solid #ddd;
    margin-bottom: 12px;
    box-shadow: 0px 0px 2px 0px;
}
.table-btn.search .form-group{
    position: relative;
}
.table-btn.search .form-group:before {
    position: absolute;
    font-family: 'FontAwesome';
    top: 5px;
    left: 7px;
    color: #9c9c9c;
    content: "\f002";
  }

.search-area
{
    font-style: italic;
    margin-top: 30px;
    margin-bottom: 30px;
    font-size: 30px;
}
input[type="text"] 
{
    font-style:italic;
}
.table-btn .btn-excel, .table-btn .btn-pdf{
    margin-bottom: 16px;
    background-color: #357080;
    color: #FFF;
    border-radius: 0;
}
.btn-excel:focus, .btn-excel:active{
    outline: none !important;
    border: none !important;
}
.fa-sorting{
    cursor: pointer;
}
.table-btn {
    display: inline-block;
    float: right;
    margin: 0 5px;
    // margin-bottom: 15px;
}

table thead {
    background-color: #555 !important;
}
table thead th {
    vertical-align: middle;
    text-align: center;
    flex: 0 0 100px!important;
    padding: .35rem;
    color: #FFF;
    font-weight: normal;
    // min-width: 100px;
}
tr.disabled td {
    background-color: #cacaca !important;
}
.table-bordered td, .table-bordered th {
    border: 3px solid #FFF;
}
table tbody td {
    background-color: #efefef;
}
table tbody td.no-edit {
    background-color: #e5fff1;
}
.remark-box.not-filled {
    background-color: #f7baba;
}
.status-circle {
    width: 20px;
    height: 20px;
    border-radius: 50%;
    border: 2px solid #00853d;
    background-color: #FFF;
    display: inline-block;
}
td.Indicator div {
    text-align: left;
}
.status-circle.red {
    border-color: #F00;
    background-color: #F00;
}
.status-circle.green {
    background-color: #00853d;
}
.status-circle.grey {
    border: 2px solid #397a46;
    background-color: #fff;
}
.Data.Value input {
    width: 70px;
    text-align: center;
    font-size: 22px;
    border: 1px solid #CCC;
}

.action-buttons {
    text-align: center;
}
.submit-btn {
    background-color: #333;
    border: 1px solid #333;
    color: #fff;
    padding: 5px 20px 5px 20px;
    cursor: pointer;
    border-radius: 2px;
    margin-bottom: 40px;
}